<%= form_for(@album) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
    <%= f.label :summary %>
    <%= f.text_area :summary, class: 'form-control',placeholder: "Compose summary" %>
    <%= f.label :csize %>
    <%= f.text_field :csize, class: 'form-control',placeholder: "s m l xl" %>
    <%= f.label :ussize %>
    <%= f.text_field :ussize, class: 'form-control' ,placeholder:"8 10 12 14"%>
    <%= f.label :brand %>
    <%= f.text_field :brand, class: 'form-control' ,placeholder:"Your Brand name"%>
    <%= f.label :fullname %>
    <%= f.text_field :fullname, class: 'form-control' ,placeholder:"Fullname"%>
     <%= f.label :dname %>
     <%= f.text_field :dname, class: 'form-control' ,placeholder:"Short name for description"%>
     <%= f.label :description %>
     <%= f.text_area :description, class: 'form-control',placeholder: "Bust Waist|80 90|90 100" %>
     <%= f.label :dnote %>
     <%= f.text_area :dnote, class: 'form-control',placeholder: "Note" %>






    <%= f.submit yield(:button_text), class: "btn btn-primary" %>
 <% end %>
