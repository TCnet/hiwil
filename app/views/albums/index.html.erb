<% provide(:title, 'My albums') %>

<div class="row">
  <aside class="col-md-3 albums_menu">
   <%= render 'shared/album_menu' %>
  </aside>
  <div class="col-md-9">

    <div class="row albums_search">
      <ul>
        <% @category.each do |f|%>
        <li>

      <%= link_to f,albums_path(:q => f) %>
</li>
      <% end %>
      </ul>
      </div>


   <div class="row albums">
     <%= render @albums %>
     </div>
  <%= will_paginate %>

  </div>

</div>
